app.service('searchService',function($http){
	
	
	this.search=function(searchMap){
		var hhh={id:3333333,name:"hh",firstChar:"oo"};
		var hhh1={id:33333332,name:"hh1",firstChar:"oo1"};
		var sc=new Array();
		sc[0]=hhh;
		sc[1]=hhh1;
		return $http.post('itemsearch/search.do',sc);
	}
	
});