app.controller('searchController',function($scope,searchService){
	
	//搜索
	$scope.search=function(){
		searchService.search($scope.searchMap).success(
			function(response){
				/*$scope.resultMap=response;*/
				for(var i=0;i<response.length;i++){
					alert(response[i]);
				}
			}
		);		
	}
	
	
});